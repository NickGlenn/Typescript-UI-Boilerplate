{
  "compilerOptions": {
    "target": "es5",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "noImplicitReturns": true,
    "jsx": "react",
    "lib": [
      "dom",
      "es2015",
      "es2016",
      "es2017",
      "es2018"
    ]
  },
  "include": [
    "./src"
  ],
  "exclude": [
    "node_modules"
  ]
}